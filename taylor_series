import numpy as np
import sympy as sym
def f(x):
    return np.tan(x)
def taylor_series(x, fun, x0, n):
    y = sym.Symbol('y')
    result = 0
    current = 1
    for k in range(1, n+1):
        #result += current * float(fun(x0))  ???
        current *= (x-x0) / k
        fun = fun.diff(y)
        print(fun)
    return result
x = 10
x0 = 0
h = sym.tan
n = 7
print(float(f(x)))
y = sym.Symbol('y')
print(taylor_series(x, h(y), x0, n))
